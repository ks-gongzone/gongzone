<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gongzone.central.sample.mapper.SampleMemberMapper">

  <!-- Result Mapping -->
  <resultMap id="SampleMemberResultMap" type="SampleMember">
    <id property="memberNo" column="m_no"/>
    <result property="memberId" column="m_id"/>
  </resultMap>

  <select id="findSampleMemberByMemberNo" parameterType="String" resultMap="SampleMemberResultMap">
    SELECT m_no, m_id
    FROM member
    WHERE m_no = #{memberNo};
  </select>

</mapper>
